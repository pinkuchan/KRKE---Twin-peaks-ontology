:LauraDreamTheory a :LauraDream,
        :Theory,
        owl:NamedIndividual ;
    rdfs:label "Laura Dream Theory" ;
    :asserts :LD_BOB_StatusClaim,
        :LD_BlackLodge_StatusClaim,
        :LD_Cooper_StatusClaim,
        :LD_Doppelganger_StatusClaim,
        :LD_Laura_SingleDreamerClaim,
        :LD_Laura_StatusClaim,
