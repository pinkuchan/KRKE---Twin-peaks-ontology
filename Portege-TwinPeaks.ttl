@prefix : <http://example.org/dream-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/dream-ontology> a owl:Ontology ;
    :competencyQuestion "Under CooperDreamTheory, what RealityStatus is assigned to BOB?",
        "Where do two theories disagree on the RealityStatus of the same narrative entity?",
        "Which claims are about LauraMurderEvent?",
        "Which claims are asserted by each theory?",
        "Which claims identify a dreamer, and who is the dreamer?",
        "Which entities are assigned projection in any theory?",
        "Which entities are uncertain under RealityOverlapTheory?",
        "Which theories classify TwinPeaksTown as dream?" ;
    rdfs:comment "This ontology models interpretive theories about Twin Peaks by separating narrative entities (Person, Place, Event, Realm) from theory-scoped claims. A Theory asserts one or more Claims. Each Claim isAbout a NarrativeEntity and may assign a RealityStatus or specify a dreamer (DreamerClaim). The design supports competing interpretations without forcing a single global truth." ;
    owl:versionInfo "v1.0" .

:BinaryStatusClaimShape sh:property [ sh:class :RealityStatus ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path :assignsStatus ] ;
    sh:targetClass :BinaryStatusClaim .

:ClaimAboutShape sh:property [ sh:class :NarrativeEntity ;
            sh:minCount 1 ;
            sh:path :isAbout ] ;
    sh:targetClass :Claim .

:DistributedDreamersClaimShape sh:property [ sh:class :Person ;
            sh:minCount 2 ;
            sh:path :hasDreamer ] ;
    sh:targetClass :DistributedDreamersClaim .

:DreamerExistenceClaim a owl:Class ;
    rdfs:label "dreamer existence claim" ;
    rdfs:comment "Dreamer Existence Claim is a concept in the ontology." ;
    rdfs:subClassOf :DreamerClaim .

:HybridStatusClaimShape sh:property [ sh:class :RealityStatus ;
            sh:minCount 2 ;
            sh:path :assignsStatus ] ;
    sh:targetClass :HybridStatusClaim .

:SingleDreamerClaimShape sh:property [ sh:class :Person ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path :hasDreamer ] ;
    sh:targetClass :SingleDreamerClaim .

:StatusShiftClaimShape sh:property [ sh:class :RealityStatus ;
            sh:minCount 2 ;
            sh:path :assignsStatus ] ;
    sh:targetClass :StatusShiftClaim .

:asserts a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "asserts" ;
    rdfs:comment "Links a Theory to the Claims it asserts." ;
    rdfs:domain :Theory ;
    rdfs:range :Claim .

:competencyQuestion a owl:AnnotationProperty ;
    rdfs:label "competency question" ;
    rdfs:comment "Competency questions the ontology should be able to answer." .

:ruleCategory a owl:DatatypeProperty ;
    rdfs:label "rule category" ;
    rdfs:comment "Category label for a theory rule claim." ;
    rdfs:domain :TheoryRuleClaim ;
    rdfs:range xsd:string .

:ruleText a owl:DatatypeProperty ;
    rdfs:label "rule text" ;
    rdfs:comment "Human-readable text of a theory rule claim." ;
    rdfs:domain :TheoryRuleClaim ;
    rdfs:range xsd:string .

sh:class a owl:AnnotationProperty .

sh:maxCount a owl:AnnotationProperty .

sh:minCount a owl:AnnotationProperty .

sh:path a owl:AnnotationProperty .

sh:property a owl:AnnotationProperty .

sh:targetClass a owl:AnnotationProperty .

:BinaryStatusClaim a owl:Class ;
    rdfs:label "binary status claim" ;
    rdfs:comment "Binary Status Claim is a concept in the ontology." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :RealityStatus ;
            owl:onProperty :assignsStatus ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :RealityStatusClaim ;
    owl:disjointWith :HybridStatusClaim,
        :StatusShiftClaim .

:CD_Rule_01 a :Claim,
        :TheoryRuleClaim,
        owl:NamedIndividual ;
    :ruleCategory "ContradictionHandling" ;
    :ruleText "Inconsistencies and identity splits are interpreted as dream distortions rather than logical errors" .

:CD_Rule_02 a :Claim,
        :TheoryRuleClaim,
        owl:NamedIndividual ;
    :ruleCategory "TemporalAssumptions" ;
    :ruleText "Temporal instability, looping, and fragmented continuity are natural consequences of dream-based narration." .

:CooperDream a owl:Class ;
    rdfs:label "cooper-dream" ;
    rdfs:comment "Cooper Dream is a specialized kind of Theory." ;
    rdfs:subClassOf :Theory .

:DistributedDreamersClaim a owl:Class ;
    rdfs:label "distributed dreamers claim" ;
    rdfs:comment "Distributed Dreamers Claim is a concept in the ontology." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onClass :Person ;
            owl:onProperty :hasDreamer ],
        :DreamerClaim ;
    owl:disjointWith :SingleDreamerClaim .

:LD_Rule_01 a :Claim,
        :TheoryRuleClaim,
        owl:NamedIndividual ;
    :ruleCategory "ContradictionHandling" ;
    :ruleText "Apparent contradictions, repetitions, or inconsistencies are resolved as dream effects such as repression, substitution, or symbolic displacement." .

:LD_Rule_02 a :Claim,
        :TheoryRuleClaim,
        owl:NamedIndividual ;
    :ruleCategory "TemporalAssumptions" ;
    :ruleText "Time is non-linear and subjective; looping, forgetting, and narrative resets are expected features of the dream structure." .

:LauraDream a owl:Class ;
    rdfs:label "laura-dream" ;
    rdfs:comment "Laura Dream is a specialized kind of Theory." ;
    rdfs:subClassOf :Theory .

:RO_Rule_01 a :Claim,
        :TheoryRuleClaim,
        owl:NamedIndividual ;
    :ruleCategory "ContradictionHandling" ;
    :ruleText "Contradictions and paradoxes are meaningful features of the narrative rather than problems requiring resolution." .

:RealityOverlap a owl:Class ;
    rdfs:label "reality overlap" ;
    rdfs:comment "Reality Overlap is a specialized kind of Theory." ;
    rdfs:subClassOf :Theory .

:non-existant a owl:Class,
        owl:NamedIndividual ;
    rdfs:label "non-existant" ;
    rdfs:comment "non existant is a specialized kind of Reality Status." ;
    rdfs:subClassOf :RealityStatus .

:Event a owl:Class ;
    rdfs:label "event" ;
    rdfs:comment "Event is a specialized kind of Narrative Entity." ;
    rdfs:subClassOf :NarrativeEntity ;
    owl:disjointWith :Person,
        :Place,
        :Realm .

:RO_Rule_02 a :Claim,
        :TheoryRuleClaim,
        owl:NamedIndividual ;
    :ruleCategory "TemporalAssumptions" ;
    :ruleText "Narrative time is not globally linear. While everyday sequences may appear chronological, dreamâ€“reality merging produces temporal instability, repetition, and discontinuity." .

:isAbout a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "is about" ;
    rdfs:comment "Links a Claim to the NarrativeEntity it concerns." ;
    rdfs:domain :Claim ;
    rdfs:range :NarrativeEntity .

:CD_BOB_StatusClaim a :RealityStatusClaim,
