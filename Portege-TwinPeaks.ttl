    rdfs:comment "Individual of class Event." ;
    rdfs:seeAlso :CD_Doppelganger_StatusShiftClaim,
        :LD_Doppelganger_StatusClaim,
        :RO_Doppelganger_HybridStatusClaim .

:DreamerClaim a owl:Class ;
    rdfs:label "dreamer claim" ;
    rdfs:comment "Dreamer Claim is a specialized kind of Claim." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasDreamer ;
            owl:someValuesFrom :Person ],
