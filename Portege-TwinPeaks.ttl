        :LD_Laura_StatusClaim,
        :RO_Laura_StatusClaim .

:RealityOverlapTheory a :RealityOverlap,
        :Theory,
        owl:NamedIndividual ;
    rdfs:label "Reality Overlap Theory" ;
    :asserts :RO_BOB_StatusClaim,
        :RO_BlackLodge_StatusClaim,
        :RO_Cooper_StatusClaim,
        :RO_Doppelganger_HybridStatusClaim,
        :RO_Laura_StatusClaim,
        :RO_Leland_StatusClaim,
        :RO_Murder_HybridStatusClaim,
        :RO_Rule_01,
        :RO_Rule_02,
        :RO_TwinPeaks_StatusClaim ;
    rdfs:comment "Individual of class RealityOverlap." ;
    rdfs:seeAlso :RO_BOB_StatusClaim,
        :RO_BlackLodge_StatusClaim,
        :RO_Cooper_StatusClaim,
        :RO_Doppelganger_HybridStatusClaim,
        :RO_Laura_StatusClaim,
        :RO_Leland_StatusClaim,
        :RO_Murder_HybridStatusClaim,
        :RO_TwinPeaks_StatusClaim .

:TheoryRuleClaim a owl:Class ;
    rdfs:label "theory rule claim" ;
    rdfs:comment "Theory Rule Claim is a specialized kind of Claim." ;
    rdfs:subClassOf :Claim .

