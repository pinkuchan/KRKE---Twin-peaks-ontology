    rdfs:comment "Individual of class Place." ;
    rdfs:seeAlso :CD_TwinPeaks_StatusClaim,
        :LD_TwinPeaks_StatusClaim,
        :RO_TwinPeaks_StatusClaim .

:NarrativeEntity a owl:Class ;
    rdfs:label "Narrative entity" .

:Theory a owl:Class ;
    rdfs:label "Theory" .

:assignsStatus a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "assigns status" ;
    rdfs:comment "Links a RealityStatusClaim to a RealityStatus value it assigns." ;
    rdfs:domain :RealityStatusClaim ;
    rdfs:range :RealityStatus .

:projection a owl:Class,
        owl:NamedIndividual ;
    rdfs:label "projection" ;
    rdfs:comment "projection is a specialized kind of Reality Status." ;
    rdfs:subClassOf :RealityStatus .

:LauraPalmer a :Person,
