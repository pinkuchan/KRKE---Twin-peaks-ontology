:Realm a owl:Class ;
    rdfs:label "realm" ;
    rdfs:comment "Realm is a specialized kind of Narrative Entity." ;
    rdfs:subClassOf :NarrativeEntity .

:SingleDreamerClaim a owl:Class ;
    rdfs:label "single dreamer claim" ;
    rdfs:comment "Single Dreamer Claim is a concept in the ontology." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Person ;
            owl:onProperty :hasDreamer ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :DreamerClaim .

:StatusShiftClaim a owl:Class ;
    rdfs:label "status shift claim" ;
    rdfs:comment "Status Shift Claim is a concept in the ontology." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onClass :RealityStatus ;
            owl:onProperty :assignsStatus ],
        :RealityStatusClaim .

:hasDreamer a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "has dreamer" ;
    rdfs:comment "Links a DreamerClaim to the Person who is asserted to be the dreamer." ;
    rdfs:domain :DreamerClaim ;
    rdfs:range :Person .

:uncertain a owl:Class,
        owl:NamedIndividual ;
    rdfs:label "uncertain" ;
    rdfs:comment "uncertain is a specialized kind of Reality Status." ;
    rdfs:subClassOf :RealityStatus .

:BOB a :Person,
        owl:NamedIndividual ;
    rdfs:label "BOB" ;
    rdfs:comment "Individual of class Person." ;
    rdfs:seeAlso :CD_BOB_StatusClaim,
        :LD_BOB_StatusClaim,
        :RO_BOB_StatusClaim .

:BlackLodge a :Realm,
        owl:NamedIndividual ;
    rdfs:label "Black Lodge" ;
    rdfs:comment "Individual of class Realm." ;
    rdfs:seeAlso :CD_BlackLodge_StatusClaim,
        :LD_BlackLodge_StatusClaim,
        :RO_BlackLodge_StatusClaim .

:CooperDoppelgangerEncounterEvent a :Event,
        owl:NamedIndividual ;
